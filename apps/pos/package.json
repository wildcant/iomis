{
  "name": "@iomis/pos",
  "version": "1.0.0",
  "main": "src/_app.tsx",
  "private": true,
  "scripts": {
    "dev": "pnpm start",
    "lint": "eslint --ext js,ts,tsx .",
    "start": "expo start",
    "start:prod": "expo start --no-dev --minify",
    "test": "jest",
    "build": "expo export --output-dir ./build --platform all",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eas-build-pre-install": "npm install --global pnpm@7.x",
    "eas-build-post-install": "pnpm run -w build"
  },
  "dependencies": {
    "@iomis/api": "*",
    "@shopify/flash-list": "1.1.0",
    "@tanstack/react-query": "^4.7.2",
    "@trpc/client": "10.0.0-proxy-beta.9",
    "@trpc/react": "10.0.0-proxy-beta.9",
    "@trpc/server": "10.0.0-proxy-beta.9",
    "expo": "^46.0.0",
    "expo-dev-client": "~1.2.1",
    "expo-status-bar": "~1.4.0",
    "expo-updates": "~0.14.5",
    "nativewind": "^2.0.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-popup-menu": "^0.16.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-web": "~0.18.9"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@iomis/eslint-config": "*",
    "@testing-library/react-native": "11.0.0",
    "@types/jest": "^27.0.0",
    "@types/react": "^18.0.21",
    "@types/react-native": "~0.69.6",
    "babel-preset-expo": "^9.2.0",
    "eslint": "^8.24.0",
    "eslint-config-universe": "^11.1.0",
    "jest": "^27.0.0",
    "jest-expo": "^46.0.1",
    "react-test-renderer": "18.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@iomis/eslint-config",
      "eslint-config-universe"
    ],
    "ignorePatterns": [
      ".expo-shared",
      ".turbo",
      "assets",
      "build",
      "node_modules"
    ],
    "rules": {
      "import/order": "off"
    }
  }
}
