{
  "cli": {
    "version": ">= 2.1.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "monorepo": {
      "cache": {
        "key": "turbo",
        "customPaths": ["../../node_modules/.cache/turbo"]
      }
    },
    "development": {
      "env": {
        "API_URL": "https://iomis-pos-restaurant-admin-8a11tbb4r-cwirl.vercel.app"
      },
      "extends": "monorepo",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "API_URL": "https://iomis-pos-restaurant-admin-8a11tbb4r-cwirl.vercel.app"
      },
      "extends": "monorepo",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      }
    }
  }
}
