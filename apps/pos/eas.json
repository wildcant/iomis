{
  "cli": {
    "version": ">= 2.1.0",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "monorepo": {
      "cache": {
        "key": "turbo",
        "customPaths": ["../../node_modules/.cache/turbo"]
      }
    },
    "development": {
      "extends": "monorepo",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "extends": "monorepo",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
